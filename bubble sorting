#include <bits/stdc++.h> 
void bubbleSort(vector<int>& arr, int n)
{    
    int  temp;
    for (int i=1; i<n; i++){
        for (int j=0; j<n; j++){
          if (arr[j] > arr[i]) {
            //  swap(arr[j], arr[i]);
            temp = arr[j];
            arr[j] = arr[i];
            arr[i] = temp;
            
          }
        }
    }
}
