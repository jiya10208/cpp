#include <bits/stdc++.h> 
void selectionSort(vector<int>& arr, int n)
{   
    int  temp;
    for (int i=0; i<n; i++){
        for (int j=i; j<n; j++){
            if (arr[j]<arr[i]){
            //  swap(arr[j], arr[i]);
                temp=arr[j];
                arr[j]=arr[i];
                arr[i]=temp;
            }
        }
    }
    // Write your code here.
}
